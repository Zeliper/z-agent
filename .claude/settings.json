{
  "hooks": {
    "PreToolUse": [],
    "PostToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "echo '[z-agent] Bash command executed'"
          }
        ]
      }
    ],
    "Notification": []
  },
  "permissions": {
    "allow": [
      "Bash(python*:*)",
      "Bash(python3*:*)",
      "Read(.z-agent/**)",
      "Write(.z-agent/**)",
      "Edit(.z-agent/**)"
    ],
    "deny": []
  }
}
